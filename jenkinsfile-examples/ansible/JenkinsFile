pipeline {
    agent none
    stages {
        stage('Staging'){
            agent { label 'docker' }
            steps {
                checkout scm
                sh 'sudo apt-cache policy'
                sh 'sudo apt-get -y install libssl-dev'
                sh 'pip install -r ./jenkinsfile-examples/ansible/ansible25.requirements'
                sh 'ansible -version'
            }
        }
    }
}
