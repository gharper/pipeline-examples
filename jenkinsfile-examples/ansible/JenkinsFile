pipeline {
    agent none
    stages {
        stage('Staging'){
            agent { label 'docker' }
            steps {
                checkout scm
                script {
                    sh 'sudo apt-get update'
                    sh 'python --version'
                    sh 'pip install ansible==2.5.2'
                    sh 'ansible -version'
                }
            }
        }
    }
}
